<!-- 代码清单6-12 视图entries-->

<!DOCTYPE html>
<html>
    <head>
        <title><%= title %></title>
        <link rel="stylesheet" href="/stylesheets/style.css"/>
    </head>
    <body>
        <% include menu %>
        <% entries.forEach((entry)=>{ %>
            <div class="entry">
                <h3><%= entry.title %></h3>
                <p><%= entry.body %></p>
                <p>Post by <%= entry.username %></p>
            </div>
        <% }) %>
    </body>
</html>